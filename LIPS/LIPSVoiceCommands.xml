<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="AdventureWorksCommandSet_en-us">
    <AppName> LIPS </AppName>
    <Example> Can I Park </Example>


    <Command Name="CanIPark">
      <Example>Can I Park?</Example>
      <ListenFor RequireAppName="BeforeOrAfterPhrase"> can i park</ListenFor>
      <ListenFor RequireAppName="BeforeOrAfterPhrase"> can i park at {office}</ListenFor>
      <ListenFor RequireAppName="BeforeOrAfterPhrase"> is there a parking place</ListenFor>
      <ListenFor RequireAppName="BeforeOrAfterPhrase"> is there a parking place at {office}</ListenFor>
      <Feedback>Getting free parking places</Feedback>
      <VoiceCommandService Target="AdventureWorksVoiceCommandService"/>
    </Command>

    <PhraseList Label="office">
      <Item>Outlook</Item>
    </PhraseList>

  </CommandSet>
</VoiceCommands>